{
  "name": "node-starter",
  "version": "1.0.0",
  "description": "Node server starter project",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile",
    "build:test": "NODE_ENV=test npm-run-all clean compile",
    "build:production": "NODE_ENV=production npm-run-all clean compile",
    "compile": "tsc",
    "commit": "cz",
    "clean": "rimraf dist",
    "lint": "./node_modules/.bin/eslint . --ext .ts --resolve-plugins-relative-to .",
    "release": "standard-version",
    "copy-assets": "npx shx cp -Rf src/assets ./dist && npx shx cp -Rf src/robots.txt ./dist/robots.txt",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "docker:serve-test": "NODE_ENV=test docker compose up",
    "docker:serve-production": "NODE_ENV=production docker compose up"
  },
  "author": "Lars Rye Jeppesen",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.2.223",
    "@swc/helpers": "^0.4.3",
    "@types/body-parser": "^1.19.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "link-module-alias": "^1.2.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "mathjs": "^11.4.0",
    "module-alias": "^2.2.2",
    "validator": "^13.7.0"
  }
}
